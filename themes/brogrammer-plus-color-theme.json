{
  "name": "Brogrammer Plus",
  "colors": {
    // Base Colors
    // https://code.visualstudio.com/api/references/theme-color#base-colors
    "focusBorder": "#ffffff00",
    // Editor colors
    "editor.background": "#212121",
    "editor.foreground": "#E0E0E0",
    "editorCursor.foreground": "#ffffff",
    "editor.selectionBackground": "#FCBC2E20",
    "editorRuler.foreground":"#FCBC2E78",
    // Minimap
    // https://code.visualstudio.com/api/references/theme-color#minimap
    "minimap.selectionHighlight": "#FCBC2E20",
    // Title Bar colors
    // https://code.visualstudio.com/api/references/theme-color#title-bar-colors
    "titleBar.activeBackground": "#353535",
    "titleBar.activeForeground": "#EEEEEE",
    "titleBar.inactiveBackground": "#353535",
    "titleBar.inactiveForeground": "#606060",
    // Editor Groups & Tabs
    // https://code.visualstudio.com/api/references/theme-color#editor-groups-tabs
    "editorGroupHeader.tabsBackground": "#212121",
    "tab.activeBackground": "#212121",
    "tab.activeBorder": "#FCBC2E",
    "tab.activeForeground": "#C6C6C6",
    "tab.inactiveBackground": "#212121",
    "tab.inactiveForeground": "#C6C6C6",
    // Side Bar colors
    // https://code.visualstudio.com/api/references/theme-color#side-bar
    "sideBar.background": "#353535",
    "sideBar.foreground": "#EEEEEE",
    "sideBarSectionHeader.background": "#353535",
    "sideBarSectionHeader.foreground": "#EEEEEE",
    // Lists and trees
    // https://code.visualstudio.com/api/references/theme-color#lists-and-trees
    "list.activeSelectionBackground":"#FB5F5720",
    "list.hoverBackground":"#FB5F5710",

    // Status Bar colors
    // https://code.visualstudio.com/api/references/theme-color#status-bar-colors
    // "statusBar.background": "#3498db",
    // "statusBar.foreground": "#ffffff",
    // Activity Bar colors
    // https://code.visualstudio.com/api/references/theme-color#activity-bar
    "activityBar.background": "#353535",
    "activityBar.foreground": "#EEEEEE",
    "activityBar.inactiveForeground": "#808080",
    "activityBar.activeBorder": "#FB5F57",
    "activityBarBadge.background":"#FB5F57",
    "activityBarBadge.foreground":"#ffffff",
    // Panel Colors
    // https://code.visualstudio.com/api/references/theme-color#panel-colors
    "panel.background": "#131313",
    "panelTitle.activeForeground": "#C6C6C6",
    "panelTitle.inactiveForeground": "#C6C6C6",
    "panelTitle.activeBorder":"#28C840",
    // Terminal Colors
    // Modified from https://github.com/mbadolato/iTerm2-Color-Schemes/blob/master/vscode/Brogrammer.json
    "terminal.foreground": "#d6dbe5",
    "terminal.background": "#131313",
    "terminal.ansiBlack": "#1f1f1f",
    "terminal.ansiBlue": "#2a84d2",
    "terminal.ansiCyan": "#1081d6",
    "terminal.ansiGreen": "#2dc55e",
    "terminal.ansiMagenta": "#4e5ab7",
    "terminal.ansiRed": "#f81118",
    "terminal.ansiWhite": "#d6dbe5",
    "terminal.ansiYellow": "#ecba0f",
    "terminal.ansiBrightBlack": "#d6dbe5",
    "terminal.ansiBrightBlue": "#1081d6",
    "terminal.ansiBrightCyan": "#0f7ddb",
    "terminal.ansiBrightGreen": "#1dd361",
    "terminal.ansiBrightMagenta": "#5350b9",
    "terminal.ansiBrightRed": "#de352e",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#f3bd09",
    "terminal.selectionBackground": "#28C84015",
    "terminalCursor.foreground": "#b9b9b9"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "string.quoted.docstring"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#606060"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#6c71c4",
        "fontStyle": ""
      }
    },
    {
      "name": "Entity",
      "scope": ["entity"],
      "settings": {
        "fontStyle": "",
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "fontStyle": " bold",
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#3498db"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "fontStyle": "",
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#E0E0E0",
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Entity inherited-class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "String embedded-source",
      "scope": "string.quoted source",
      "settings": {
        "fontStyle": "",
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "String constant",
      "scope": "string constant",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "String.regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "String variable",
      "scope": "string variable",
      "settings": {
        "foreground": "#e67e22"
      }
    },
    {
      "name": "Support.function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#3498db"
      }
    },
    {
      "name": "Support.constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "C/C++ Preprocessor Line",
      "scope": "other.preprocessor.c",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "C/C++ Preprocessor Directive",
      "scope": "other.preprocessor.c entity",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "Javascript object literal separator",
      "scope": "punctuation.separator.key-value.js",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Javascript object literal key",
      "scope": "meta.object-literal.key.js",
      "settings": {
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "Meta.tag.A",
      "scope": [
        "declaration.tag",
        "declaration.tag entity",
        "meta.tag",
        "meta.tag entity"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "css tag-name",
      "scope": "meta.selector.css entity.name.tag",
      "settings": {
        "foreground": "#1abc9c"
      }
    },
    {
      "name": "css#id",
      "scope": "meta.selector.css entity.other.attribute-name.id",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "css.class",
      "scope": "meta.selector.css entity.other.attribute-name.class",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "css property-name:",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#3498db",
        "fontStyle": ""
      }
    },
    {
      "name": "css property-value;",
      "scope": [
        "meta.property-group support.constant.property-value.css",
        "meta.property-value support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#2ecc71",
        "fontStyle": " italic"
      }
    },
    {
      "name": "css @at-rule",
      "scope": "meta.preprocessor.at-rule keyword.control.at-rule",
      "settings": {
        "foreground": "#f1c40f",
        "fontStyle": " bold"
      }
    },
    {
      "name": "css additional-constants",
      "scope": [
        "meta.property-value support.constant.named-color.css",
        "meta.property-value constant"
      ],
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "css constructor.argument",
      "scope": "meta.constructor.argument.css",
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "html id",
      "scope": "entity.other.attribute-name.id.html",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "html class",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "html punctuations tags",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "CSS @import",
      "scope": "keyword.control.at-rule.import.css",
      "settings": {
        "foreground": "#f1c40f",
        "fontStyle": " bold"
      }
    },
    {
      "name": "LESS variables",
      "scope": "variable.other.less",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "LESS variables",
      "scope": "entity.other.less.mixin",
      "settings": {
        "foreground": "#9b59b6"
      }
    },
    {
      "name": "LESS units",
      "scope": "source.css.less keyword.unit.css",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "Angular",
      "scope": [
        "entity.other.attribute-name.angular.html",
        "source.angular.embedded.html"
      ],
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "HTML Entities",
      "scope": "constant.character.entity.html",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "coffee instance var",
      "scope": "variable.other.readwrite.instance.coffee",
      "settings": {
        "foreground": "#e67e22"
      }
    },
    {
      "name": "coffee brace",
      "scope": ["meta.brace.round.coffee", "meta.brace.square.coffee"],
      "settings": {
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "coffee embeded",
      "scope": "punctuation.section.embedded.coffee",
      "settings": {
        "foreground": "#5eebb8"
      }
    },
    {
      "name": "Coffee vars",
      "scope": "variable.assignment.coffee variable.assignment.coffee",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Coffee dots",
      "scope": "meta.delimiter.method.period.coffee",
      "settings": {
        "foreground": "#3cadd7"
      }
    },
    {
      "name": "Coffee curleys",
      "scope": "meta.brace.curly.coffee",
      "settings": {
        "foreground": "#1abc9c"
      }
    },
    {
      "name": "Doctype/XML Processing",
      "scope": [
        "meta.tag.sgml.doctype.xml",
        "declaration.sgml.html declaration.doctype",
        "declaration.sgml.html declaration.doctype entity",
        "declaration.sgml.html declaration.doctype string",
        "declaration.xml-processing",
        "declaration.xml-processing entity",
        "declaration.xml-processing string",
        "doctype"
      ],
      "settings": {
        "foreground": "#3c444d"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // https://code.visualstudio.com/api/language-extensions/semantic-highlight-guide#semantic-token-scope-map
    "namespace": {
      // For identifiers that declare or reference a namespace, module, or package.
      "foreground": "#2ecc71"
    },
    "type": {
      "foreground": "#2ecc71"
    },
    "type.defaultLibrary": {
      "foreground": "#3498db"
    },
    "struct": {
      "foreground": "#3498db"
    },
    "class": {
      "foreground": "#2ecc71"
    },
    "class.defaultLibrary": {
      "foreground": "#3498db"
    },
    "interface": {
      "foreground": "#2ecc71"
    },
    "enum": {
      "foreground": "#2ecc71"
    },
    "function": {
      "foreground": "#2ecc71"
    },
    "function.defaultLibrary": {
      "foreground": "#3498db"
    },
    "method": {
      "foreground": "#2ecc71"
    },
    "macro": {
      "foreground": "#2ecc71"
    },
    "module": {
      // for Python
      "foreground": "#2ecc71"
    },
    "variable": {
      "foreground": "#E0E0E0"
    },
    "variable.readonly": {
      "foreground": "#E0E0E0"
    },
    "variable.readonly.defaultLibrary": {
      "foreground": "#3498db"
    },
    "parameter": {
      "foreground": "#e67e22"
    },
    "property": {
      "foreground": "#E0E0E0"
    },
    "property.readonly": {
      "foreground": "#E0E0E0"
    },
    "enumMember": {
      "foreground": "#E0E0E0"
    },
    "event": {
      "foreground": "#E0E0E0"
    },
    //TypeHint Patch, keep it to the bottom
    "*.typeHint": "#606060"
  }
}
