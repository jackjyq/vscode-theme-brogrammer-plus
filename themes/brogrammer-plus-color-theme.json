{
  "name": "Brogrammer Plus",
  "colors": {
    // Base Colors
    // https://code.visualstudio.com/api/references/theme-color#base-colors
    "focusBorder": "#ffffff00",
    // Editor colors
    "editor.background": "#121212",
    "editor.foreground": "#ffffffde",
    "editorCursor.foreground": "#ffffff",
    "editorRuler.foreground": "#ffffff99",
    "editorLineNumber.activeForeground": "#ffffffde",
    "editorLineNumber.foreground": "#ffffff99",
    // Peek view colors
    // https://code.visualstudio.com/api/references/theme-color#peek-view-colors
    "peekView.border": "#383838",
    "peekViewEditor.background": "#2E2E2E",
    "peekViewResult.background": "#323232",
    "peekViewResult.fileForeground": "#ffffffde",
    "peekViewResult.lineForeground": "#ffffffde",
    "peekViewTitle.background": "#383838",
    "peekViewTitleLabel.foreground": "#ffffffde",
    "peekViewTitleDescription.foreground": "#ffffff99",
    // Title Bar colors
    // https://code.visualstudio.com/api/references/theme-color#title-bar-colors
    "titleBar.activeBackground": "#383838",
    "titleBar.activeForeground": "#ffffffde",
    "titleBar.inactiveBackground": "#383838",
    "titleBar.inactiveForeground": "#ffffff99",
    // Status Bar colors
    // https://code.visualstudio.com/api/references/theme-color#status-bar-colors
    "statusBar.background": "#383838",
    "statusBar.foreground": "#ffffffde",
    "statusBar.focusBorder": "#ffffff00",
    "statusBar.border": "#ffffff00",
    "statusBar.debuggingBackground": "#383838",
    "statusBar.debuggingForeground": "#ffffffde",
    "statusBar.debuggingBorder": "#ffffff00",
    "statusBar.noFolderBackground": "#383838",
    "statusBar.noFolderForeground": "#ffffffde",
    "statusBar.noFolderBorder": "#ffffff00",
    // Debug Icons colors
    // https://code.visualstudio.com/api/references/theme-color#editor-groups-tabs
    "debugConsole.errorForeground": "#e74c3c",
    "debugConsole.warningForeground": "#f1c40f",
    "debugConsole.infoForeground": "#2ecc71",
    "debugConsole.sourceForeground": "#ffffffde",
    // Editor Groups & Tabs
    // https://code.visualstudio.com/api/references/theme-color#editor-groups-tabs
    "editorGroupHeader.tabsBackground": "#262626",
    "tab.activeBackground": "#323232",
    "tab.activeForeground": "#ffffffde",
    "tab.inactiveBackground": "#262626",
    "tab.inactiveForeground": "#ffffffde",
    "tab.activeModifiedBorder": "#f1c40f",
    "tab.hoverBackground": "#323232",
    // Breadcrumbs colors
    // https://code.visualstudio.com/api/references/theme-color#breadcrumbs-colors
    "breadcrumb.background": "#323232",
    "breadcrumb.foreground": "#ffffff99",
    // Side Bar colors
    // https://code.visualstudio.com/api/references/theme-color#side-bar
    "sideBar.background": "#282828",
    "sideBar.foreground": "#ffffffde",
    "sideBarSectionHeader.background": "#323232",
    "sideBarSectionHeader.foreground": "#ffffffde",
    // Activity Bar colors
    // https://code.visualstudio.com/api/references/theme-color#activity-bar
    "activityBar.background": "#383838",
    "activityBar.foreground": "#ffffffde",
    "activityBar.inactiveForeground": "#ffffff99",
    "activityBar.activeBorder": "#ffffffde",
    "activityBarBadge.background": "#e74c3c",
    "activityBarBadge.foreground": "#ffffffde",
    // Panel Colors
    // https://code.visualstudio.com/api/references/theme-color#panel-colors
    "panel.background": "#282828",
    "panelTitle.activeForeground": "#ffffffde",
    "panelTitle.inactiveForeground": "#ffffff99",
    "panelTitle.activeBorder": "#ffffffde",
    // Terminal Colors
    "terminal.foreground": "#fbfbfb",
    "terminal.background": "#121212",
    "terminal.ansiBlack": "#121212",
    "terminal.ansiBlue": "#3498db",
    "terminal.ansiCyan": "#3cc9d6",
    "terminal.ansiGreen": "#2ecc71",
    "terminal.ansiMagenta": "#6c71c4",
    "terminal.ansiRed": "#e74c3c",
    "terminal.ansiWhite": "#fbfbfb",
    "terminal.ansiYellow": "#f1c40f",
    "terminal.ansiBrightBlack": "#373737",
    "terminal.ansiBrightBlue": "#48c0ff",
    "terminal.ansiBrightCyan": "#51f1ff",
    "terminal.ansiBrightGreen": "#3bfb9d",
    "terminal.ansiBrightMagenta": "#9ea7ff",
    "terminal.ansiBrightRed": "#ff5542",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#ffd60d",
    "terminal.selectionBackground": "#264f78",
    "terminalCursor.foreground": "#ffffff"
  },
  "tokenColors": [
    // Base Colors
    {
      "name": "Comment",
      "scope": ["comment", "string.quoted.docstring"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffffff99"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Entity",
      "scope": ["entity"],
      "settings": {
        "fontStyle": "",
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "fontStyle": " bold",
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#3cc9d6"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Entity inherited-class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "String embedded-source",
      "scope": "string.quoted source",
      "settings": {
        "fontStyle": "",
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "String constant",
      "scope": "string constant",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "String.regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "String variable",
      "scope": "string variable",
      "settings": {
        "foreground": "#e67e22"
      }
    },
    {
      "name": "Support.function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#3498db"
      }
    },
    {
      "name": "Support.constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "C/C++ Preprocessor Line",
      "scope": "other.preprocessor.c",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "C/C++ Preprocessor Directive",
      "scope": "other.preprocessor.c entity",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "Javascript object literal separator",
      "scope": "punctuation.separator.key-value.js",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Javascript object literal key",
      "scope": "meta.object-literal.key.js",
      "settings": {
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "Meta.tag.A",
      "scope": [
        "declaration.tag",
        "declaration.tag entity",
        "meta.tag",
        "meta.tag entity"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "css tag-name",
      "scope": "meta.selector.css entity.name.tag",
      "settings": {
        "foreground": "#1abc9c"
      }
    },
    {
      "name": "css#id",
      "scope": "meta.selector.css entity.other.attribute-name.id",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "css.class",
      "scope": "meta.selector.css entity.other.attribute-name.class",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "css property-name:",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#3498db",
        "fontStyle": ""
      }
    },
    {
      "name": "css property-value;",
      "scope": [
        "meta.property-group support.constant.property-value.css",
        "meta.property-value support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#2ecc71",
        "fontStyle": " italic"
      }
    },
    {
      "name": "css @at-rule",
      "scope": "meta.preprocessor.at-rule keyword.control.at-rule",
      "settings": {
        "foreground": "#f1c40f",
        "fontStyle": " bold"
      }
    },
    {
      "name": "css additional-constants",
      "scope": [
        "meta.property-value support.constant.named-color.css",
        "meta.property-value constant"
      ],
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "css constructor.argument",
      "scope": "meta.constructor.argument.css",
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "markdown heading",
      "scope": [
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#e74c3c"
      }
    },    {
      "name": "markdown puncuation",
      "scope": [
        "punctuation.definition.markdown",
      ],
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "html id",
      "scope": "entity.other.attribute-name.id.html",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "html class",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#2ecc71"
      }
    },
    {
      "name": "html punctuations tags",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "CSS @import",
      "scope": "keyword.control.at-rule.import.css",
      "settings": {
        "foreground": "#f1c40f",
        "fontStyle": " bold"
      }
    },
    {
      "name": "LESS variables",
      "scope": "variable.other.less",
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "LESS variables",
      "scope": "entity.other.less.mixin",
      "settings": {
        "foreground": "#9b59b6"
      }
    },
    {
      "name": "LESS units",
      "scope": "source.css.less keyword.unit.css",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "Angular",
      "scope": [
        "entity.other.attribute-name.angular.html",
        "source.angular.embedded.html"
      ],
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "HTML Entities",
      "scope": "constant.character.entity.html",
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "coffee instance var",
      "scope": "variable.other.readwrite.instance.coffee",
      "settings": {
        "foreground": "#e67e22"
      }
    },
    {
      "name": "coffee brace",
      "scope": ["meta.brace.round.coffee", "meta.brace.square.coffee"],
      "settings": {
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "coffee embeded",
      "scope": "punctuation.section.embedded.coffee",
      "settings": {
        "foreground": "#5eebb8"
      }
    },
    {
      "name": "Coffee vars",
      "scope": "variable.assignment.coffee variable.assignment.coffee",
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "Coffee dots",
      "scope": "meta.delimiter.method.period.coffee",
      "settings": {
        "foreground": "#3cadd7"
      }
    },
    {
      "name": "Coffee curleys",
      "scope": "meta.brace.curly.coffee",
      "settings": {
        "foreground": "#1abc9c"
      }
    },
    {
      "name": "Doctype/XML Processing",
      "scope": [
        "meta.tag.sgml.doctype.xml",
        "declaration.sgml.html declaration.doctype",
        "declaration.sgml.html declaration.doctype entity",
        "declaration.sgml.html declaration.doctype string",
        "declaration.xml-processing",
        "declaration.xml-processing entity",
        "declaration.xml-processing string",
        "doctype"
      ],
      "settings": {
        "foreground": "#3c444d"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // https://code.visualstudio.com/api/language-extensions/semantic-highlight-guide#semantic-token-scope-map
    "namespace": {
      // For identifiers that declare or reference a namespace, module, or package.
      "foreground": "#2ecc71"
    },
    "type": {
      "foreground": "#2ecc71"
    },
    "type.defaultLibrary": {
      "foreground": "#3cc9d6"
    },
    "struct": {
      "foreground": "#3498db"
    },
    "class": {
      "foreground": "#2ecc71"
    },
    "interface": {
      "foreground": "#2ecc71"
    },
    "enum": {
      "foreground": "#2ecc71"
    },
    "function": {
      "foreground": "#2ecc71"
    },
    "function.defaultLibrary": {
      "foreground": "#3cc9d6"
    },
    "method": {
      "foreground": "#2ecc71"
    },
    "macro": {
      "foreground": "#2ecc71"
    },
    "variable": {
      "foreground": "#ffffffde"
    },
    "variable.readonly": {
      "foreground": "#ffffffde"
    },
    "parameter": {
      "foreground": "#e67e22"
    },
    "property": {
      "foreground": "#ffffffde"
    },
    "property.readonly": {
      "foreground": "#ffffffde"
    },
    "enumMember": {
      "foreground": "#ffffffde"
    },
    "event": {
      "foreground": "#ffffffde"
    },
    //TypeHint Patch, keep it to the bottom
    "*.typeHint": "#ffffff99"
  }
}
